kind: ConfigMap
apiVersion: v1
metadata:
  name: streamqmqsc
  namespace: $TARGET_NAMESPACE
data:
  disablesecurity.mqsc: |-
    define ql(APPQ) streamq(MY.LOG.QUEUE)
    define ql(MY.LOG.QUEUE)
    DEFINE CHANNEL($CHLCAPS) CHLTYPE(SVRCONN) TRPTYPE(TCP) SSLCAUTH(OPTIONAL) SSLCIPH('ANY_TLS12_OR_HIGHER')
    set chlauth($CHLCAPS) TYPE(BLOCKUSER) USERLIST(NOBODY)
    REFRESH SECURITY TYPE(CONNAUTH)
---
kind: Secret
apiVersion: v1
metadata:
  name: streamqqmgrcert
  namespace: $TARGET_NAMESPACE
data:
  tls.crt: 
  tls.key: 
type: Opaque
---
apiVersion: mq.ibm.com/v1beta1
kind: QueueManager
metadata:
  name: $QMname
spec:
  version: $VERSION
  license:
    accept: true
    license: L-RJON-BN7PN3
    use: "NonProduction"
  pki:
    keys:
    - name: default
      secret:
        secretName: streamqqmgrcert
        items:
          - tls.key
          - tls.crt
  web:
    enabled: true
  queueManager:
    type: ephemeral
    resources:
      limits:
        cpu: 1
        memory: "512Mi"
      requests:
        cpu: 1
        memory: "512Mi"
    mqsc:
      - configMap:
          name: streamqmqsc
          items:
            - disablesecurity.mqsc
  template:
    pod:
      containers:
        - env:
            - name: MQSNOAUT
              value: 'yes'
          name: qmgr
---
kind: Route
apiVersion: route.openshift.io/v1
metadata:
  name: mq-traffic-mq-$QMname-ibm-mq-qm
  namespace: $TARGET_NAMESPACE
spec:
  host: $CHANNEL.chl.mq.ibm.com
  to:
    kind: Service
    name: $QMname-ibm-mq
  port:
    targetPort: 1414
  tls:
    termination: passthrough
  wildcardPolicy: None
